function : void main() {
  int x = 3;
  int y = 2;
  
  int z;
  // multiplication division mod
  z = x * y;
  cout << z << endl;
  z = x / y;
  cout << z << endl;
  z = x % y;
  cout << z << endl;

  // bitwise and or xor
  z = x & y;
  cout << z << endl;
  z = x | y;
  cout << z << endl;
  z = x ^ y;
  cout << z << endl;
}
