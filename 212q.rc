structdef S{int a; int b;};
structdef S2{S * a; S * b;};

function : void foo2(S & a){
   static int i = 0;
   a.a = a.a + i; 
   a.b = a.b + ++i;
}

function : void foo (S2 & a){
   cout << a.a->a << " " << a.a->b << "; ";
   cout << a.b->a << " " << a.b->b << endl;
   foo2(*a.a);
   foo2(*a.b);
}

S2 s;
function : int main(){
   foo(s);
   foo(s);
   foo(s);

   return 0;
}
