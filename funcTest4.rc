function : int foo1()
{
  cout << "SHOULD PRINT 1" << endl;
  return 2; //return constant literal float
}

function : float foo2()
{
  cout << "SHOULD PRINT 2" << endl;
  float x = 3.3;
  return x; //return float VarSTO 
}


function : void main()
{
  int a = foo1();
  float b = foo2();

  cout << a << endl;
  cout << b << endl;

  cout << "SHOULD PRINT LAST" << endl;
}

