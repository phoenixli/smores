function : void main() {
  int x = 3;
  int y = 2;
  
  // plus and minus
  cout << x + y << endl;
  cout << x - y << endl;

  // unary plus and minus
  cout << -x << endl;
  cout << +y << endl;

  // pre and post increment
  cout << ++x + y << endl;
  cout << x << endl;
  cout << x++ + y << endl;
  cout << x << endl;

  // pre and post decrement
  cout << --x + y << endl;
  cout << x << endl;
  cout << x-- + y << endl;
  cout << x << endl;
}
