int x = 10;
int y = 12;
int z = 14;
float a = 20.0;
float b = 30.0;
float c = 20.0;

function : void main()
{
	if (4 < 5)
	{
		cout << "4 is less than 5" << endl;
	}	
	if (x == 10)
	{
		cout << "x is equal to 10" << endl;
	}
	if (y > 10)
	{
		cout << "y is greater than 10" << endl;
	}
	if (y >= 12)
	{
		cout << "y is greater than or equal to 12" << endl;
	}
	if (z < 15)
	{
		cout << "z is less than 15" << endl;
	}
	if (z <= 14)
	{
		cout << "z is less than or equal to 15" << endl;
	}
	if (y != 13)
	{
		cout << "y is not equal to 13" << endl;
	}
	if (a < b)
	{
		cout << "a is less than b" << endl;
	}
	if (a <= c)
	{
		cout << "a is less than or equal to c" << endl;
	}
	
}