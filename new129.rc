function : int foo(int x)
{
	if(x==1)
	{
		return x;
	}
	else
	{
		return foo(x-1)*x;
	}
	return x;
}

function : void main()
{
	float x = 3.12;
	cout << foo(5) << endl;
	cout << foo(6) << endl;
	cout << foo(10) << endl;

	cout << --x << endl;
	cout << ++x << endl;
	x++;
	cout << x-- << endl;
	cout << x << endl;
}
