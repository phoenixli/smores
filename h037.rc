float x = 3.2;

function : void main() {
  float y = 1.3;
  
  float z;
  // floating point addition subtraction unary signs
  x = -x;
  cout << x << endl;
  z = x + 6.4;
  cout << z << endl;
  y = +y;
  cout << y << endl;
  z = x + y;
  cout << z << endl;
