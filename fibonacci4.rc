int a;
int b;

function : int fibonacci (int n, int &x, int &y) {
    int t;

    if (n <= 0) {
        return x;
    } else {
        t = y;
        y = x + y;
        x = t;
        return fibonacci(n-1, x, y);
    }
    return 0;
}

